<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:extensions="clr-namespace:TabbyCat.Extensions"
             xmlns:tabbyCat="clr-namespace:TabbyCat"
             xmlns:viewModels="clr-namespace:TabbyCat.ViewModels"
             xmlns:languages="clr-namespace:TabbyCat.Shared.Languages;assembly=TabbyCat.Shared"
             xmlns:converters="clr-namespace:TabbyCat.Converters"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="TabbyCat.Views.MobilePersonView"
             Background="White"
             x:DataType="viewModels:MobilePersonViewModel">
    <StackPanel HorizontalAlignment="Stretch">
        
        <Button HorizontalAlignment="Right" Classes="icon"
                Margin="0,10,10,5"
                extensions:IconAttach.Icon="{x:Static tabbyCat:IconFontProvider.Setting}"
                Command="{Binding NavigationToSettingCommand}" />
        
        
    </StackPanel>
</UserControl>